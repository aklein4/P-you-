
def create(password, times):
    mus = "0"
    stds = "0"
    for i in range(len(self.data[0])):
        vals = numpy.array([])
        for set in self.data:
            vals = numpy.append(vals, set[i])
        mus += ","+str(vals.mean().round(1))
        stds += ","+str(vals.std().round(1))
    f = open("data.txt", "w")
    f.write(self.password+"\n")
    f.write(mus+"\n")
    f.write(stds)
    f.close()